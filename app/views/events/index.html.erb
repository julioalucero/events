<h1>Events</h1>

<%= render partial: 'events/search_form', locals: { search: search_params, url: events_path } %>

<div class='container-fluid'>
  <table class='table'>
    <thead>
      <tr>
        <th>Title</th>
        <th>Date</th>
        <th>Time</th>
        <th>Location</th>
        <th>Topics</th>
      </tr>
    </thead>
    <tbody>
      <% @events.each do |event| %>
        <%= render 'event', event: event %>
      <% end %>
    </tbody>
  </table>
</div>

